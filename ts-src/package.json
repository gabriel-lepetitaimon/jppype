{
  "name": "jppype",
  "version": "0.2.0-a6",
  "description": "Custom Jupyter Widgets for ProtoPype.",
  "author": {
    "name": "Gabriel Lepetit-Aimon",
    "email": "gabriel.lepetitaimon@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/gabriel-lepetitaimon/jppype",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/gabriel-lepetitaimon/jppype.git"
  },
  "keywords": [
    "jupyter",
    "jupterlab",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab-extension"
  ],
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/**/*.js",
    "dist/*.js",
    "css/*.css"
  ],
  "scripts": {
    "build": "NODE_ENV=development jlpm run build:tsc && jlpm run build:nbextension:dev && jlpm run build:labextension:dev",
    "build:prod": "NODE_ENV=production jlpm clean && jlpm run build:tsc:prod && jlpm run build:nbextension && jlpm run build:labextension",
    "build:tsc": "tsc --sourceMap",
    "build:tsc:prod": "tsc",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:nbextension": "webpack --mode=production --no-devtool",
    "build:nbextension:dev": "webpack --mode=development",
    "clean": "jlpm run clean:lib && jlpm run clean:nbextension && jlpm run clean:labextension",
    "clean:lib": "rimraf lib/ && rimraf dist/ || true",
    "clean:nbextension": "rimraf ../jppype/nbextension/index.* || true",
    "clean:labextension": "rimraf ../jppype/labextension || true",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepack": "jlpm run clean && jlpm run build:prod",
    "watch": "npm-run-all -p \"watch:*\"",
    "watch:tsc": "tsc -w --sourceMap",
    "watch:nbextension": "webpack --watch --mode=development",
    "watch:labextension": "jupyter labextension watch --development True ."
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@jupyter-widgets/base": "^6",
    "@jupyter-widgets/controls": "^5",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.7.0",
    "backbone": "^1.4.0",
    "d3": "^7.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rxjs": "^7.5.5",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.14.5",
    "@jupyterlab/application": "^4.2.0",
    "@jupyterlab/apputils": "^4.2.0",
    "@jupyterlab/builder": "^4.2.0",
    "@phosphor/application": "^1.6.0",
    "@phosphor/widgets": "^1.6.0",
    "@react-hook/resize-observer": "^2.0.2",
    "@types/backbone": "^1.4.0",
    "@types/d3": "^7.4.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^7.1.1",
    "fs-extra": "^7.0.0",
    "rimraf": "^6.0.1",
    "source-map-loader": "^1.1.3",
    "style-loader": "^1.0.0",
    "ts-loader": "^8.0.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.0.0",
    "yarn-run-all": "^3.1.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "jupyterlab": {
    "extension": "lib/labplugin",
    "outputDir": "../jppype/labextension",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    }
  }
}
